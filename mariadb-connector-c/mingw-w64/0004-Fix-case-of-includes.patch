diff --git a/libmariadb/secure/ma_schannel.h b/libmariadb/secure/ma_schannel.h
index 4e398fe..5ef2465 100644
--- a/libmariadb/secure/ma_schannel.h
+++ b/libmariadb/secure/ma_schannel.h
