+++ b/plugins/auth/sspi_common.h
@@ -29,10 +29,10 @@ POSSIBILITY OF SUCH DAMAGE.
 #define SECURITY_WIN32
 #include <windows.h>
 #include <sspi.h>
-#include <SecExt.h>
+#include <secext.h>
